{
  "name": "jexl",
  "version": "1.1.2",
  "description": "Javascript Expression Language: Powerful context-based expression parser and evaluator",
  "main": "lib/Jexl.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run lint && npm run test-cov && npm run check-cov && npm run karma",
    "lint": "eslint .",
    "karma": "karma start",
    "mocha": "mocha -R spec --recursive test",
    "test-cov": "istanbul cover _mocha -- -R spec --recursive test",
    "check-cov": "istanbul check-coverage --statements 95 --functions 92 --branches 88 --lines 95",
    "build": "browserify lib/Jexl.js -s Jexl | derequire | uglifyjs > dist/jexl.min.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TechnologyAdvice/jexl"
  },
  "keywords": [
    "JSON",
    "expression",
    "evaluator",
    "parser",
    "target",
    "context",
    "jsep",
    "filter",
    "selector"
  ],
  "author": "Tom Shawver <tom@frosteddesign.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TechnologyAdvice/jexl/issues"
  },
  "homepage": "https://github.com/TechnologyAdvice/jexl",
  "devDependencies": {
    "browserify": "^10.2.4",
    "chai": "^2.0.0",
    "chai-as-promised": "^4.2.0",
    "derequire": "^2.0.0",
    "eslint": "^0.23.0",
    "istanbul": "^0.3.8",
    "karma": "^0.12.36",
    "karma-browserify": "^4.2.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.1.10",
    "mocha": "^2.1.0",
    "uglify-js": "^2.4.23"
  }
}
